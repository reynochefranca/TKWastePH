<p id="notice"><%= notice %></p>

<h1>Listing Request History Details</h1>

<table>
  <thead>
    <tr>
      <th>Request history</th>
      <th>Edi user</th>
      <th>Manifest</th>
      <th>Fuctioction number</th>
      <th>Request value</th>
      <th>Result status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @request_history_details.each do |request_history_detail| %>
      <tr>
        <td><%= request_history_detail.request_history_id %></td>
        <td><%= request_history_detail.edi_user_id %></td>
        <td><%= request_history_detail.manifest_id %></td>
        <td><%= request_history_detail.fuctioction_number %></td>
        <td><%= request_history_detail.request_value %></td>
        <td><%= request_history_detail.result_status %></td>
        <td><%= link_to 'Show', request_history_detail %></td>
        <td><%= link_to 'Edit', edit_request_history_detail_path(request_history_detail) %></td>
        <td><%= link_to 'Destroy', request_history_detail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

