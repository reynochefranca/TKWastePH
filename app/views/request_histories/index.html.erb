<p id="notice"><%= notice %></p>

<h1>Listing Request Histories</h1>

<table>
  <thead>
    <tr>
      <th>Requested at</th>
      <th>Csv file path</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @request_histories.each do |request_history| %>
      <tr>
        <td><%= request_history.requested_at %></td>
        <td><%= request_history.csv_file_path %></td>
        <td><%= link_to 'Show', request_history %></td>
        <td><%= link_to 'Edit', edit_request_history_path(request_history) %></td>
        <td><%= link_to 'Destroy', request_history, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Request history', new_request_history_path %>
